<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NotABook.Pages.MainPages.StartPage"
                   xmlns:masterPage="clr-namespace:NotABook.Pages.MainPages"
                  xmlns:detailPages="clr-namespace:NotABook.Pages.DetailPages"
                  Title="Prog">
    <MasterDetailPage.Master>
        <masterPage:MasterPage x:Name="masterPage"/>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <detailPages:AllBooksPage/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>